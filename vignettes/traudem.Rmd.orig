```{r setup2, echo=FALSE}
setup_os <- function() {
  if (traudem:::is_ci() && traudem:::on_windows()) {
    Sys.setenv(
      PATH = paste0(
        "C:\\Program Files\\Microsoft MPI\\bin;",
        "C:\\Program Files\\GDAL;",
        "C:\\taudem;",
        Sys.getenv("PATH")
      )
    )
  }
  if (traudem:::is_ci() && !(traudem:::on_windows())) {
    Sys.setenv(TAUDEM_PATH="TauDEM/bin")
  }
}
setup_os()
